  border: 0;
  padding: 0;
  margin: 0;
}

#cboxContent, #cboxLoadedContent, #colorbox {
  box-sizing: content-box;
  -moz-box-sizing: content-box;
  -webkit-box-sizing: content-box;
}

#cboxOverlay {
  background: #fff;
}

#colorbox {
