}

#cboxBottomLeft {
  width: 25px;
  height: 25px;
  background: url({{ 'cbox_border1.png' | asset_url }}) no-repeat 0 -25px;
}

#cboxBottomCenter {
  height: 25px;
  background: url({{ 'cbox_border1.png' | asset_url }}) repeat-x 0 -75px;
}

#cboxBottomRight {
  width: 25px;
  height: 25px;
  background: url({{ 'cbox_border1.png' | asset_url }}) no-repeat -25px -25px;
}

#cboxMiddleLeft {
  width: 25px;
